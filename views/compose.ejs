<%- include("partials/header.ejs") %> 
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
    <label for="titleInput">Title</label>
    
    <input type="text" name="titleInput" id="titleInput" class="form-control" autofocus>
    </div>
    <div class="form-group">
    <label for="postInput">Post</label>
    
    <textarea class="form-control" name="postInput" id="postInput" cols="30" rows="10"></textarea>
    </div>
    
    <button onclick="window.alert('Post Submitted')" type="submit" class="btn btn-primary publish">Publish</button>
    
</form>
<%- include("partials/footer.ejs") %> 